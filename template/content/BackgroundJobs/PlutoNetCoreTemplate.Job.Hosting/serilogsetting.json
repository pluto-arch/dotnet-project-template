{
	"Serilog": {
		"Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
		"Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithProcessName", "WithThreadId", "WithThreadName" ],
		"MinimumLevel": {
			"Default": "Information",
			"Override": {
				"System": "Error",
				"Microsoft": "Error",
				"Microsoft.EntityFrameworkCore": "Error"
			}
		},
		"WriteTo": [
			{
				"Name": "Console",
				"Args": {
					"outputTemplate": "=> {Timestamp:HH:mm:ss} [{Level:u3}] {Message:j} {NewLine} {Properties:j} {NewLine}{Exception}"
				}
			},
			{
				"Name": "File",
				"Args": {
					"path": "Logs/log.txt",
					"rollingInterval": "Day",
					"outputTemplate": "=> {Timestamp:HH:mm:ss} [{Level:u3}] {Message:j} {NewLine} {Properties:j} {NewLine}{Exception}"
				}
			}
		]
	}
}